{
  "compilerOptions": {
    "sourceMap": true,
    "target": "ES6",
    "strict": true,
    "moduleResolution": "node",
    "removeComments": true,
    "esModuleInterop": true,
    "noEmitOnError": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "types": []
  }
}
